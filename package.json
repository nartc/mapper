{
  "name": "automapper",
  "version": "7.1.1",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "affected:lint": "nx affected:lint --base=main",
    "format:write": "nx format:write --base=main",
    "commit": "cz",
    "release": "dotenv release-it --",
    "contributors:init": "all-contributors init",
    "contributors:add": "all-contributors add",
    "build:all": "nx run-many --all --target=build",
    "test:all": "nx run-many --all --target=test --parallel",
    "lint:all": "nx run-many --all --target=lint",
    "publish:all": "nx run-many --target=publish --all --parallel"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nartc/mapper"
  },
  "author": {
    "name": "Chau Tran",
    "email": "nartc7789@gmail.com",
    "url": "https://nartc.me"
  },
  "description": "AutoMapper TypeScript monorepo",
  "keywords": [
    "typescript",
    "automapper",
    "mapper",
    "nx"
  ],
  "dependencies": {},
  "devDependencies": {
    "@nestjs/common": "8.2.4",
    "@nestjs/core": "8.2.4",
    "@nestjs/platform-express": "8.2.4",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.4",
    "@nrwl/cli": "13.4.3",
    "@nrwl/eslint-plugin-nx": "13.4.3",
    "@nrwl/jest": "13.4.3",
    "@nrwl/nest": "13.4.3",
    "@nrwl/node": "13.4.3",
    "@nrwl/tao": "13.4.3",
    "@nrwl/workspace": "13.4.3",
    "@release-it/bumper": "3.0.1",
    "@release-it/conventional-changelog": "3.3.0",
    "@types/jest": "27.0.3",
    "@types/jest-when": "2.7.3",
    "@types/node": "16.11.7",
    "@types/sequelize": "4.28.10",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "all-contributors-cli": "6.20.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-cli": "4.1.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "jest-createspyobj": "2.0.0",
    "jest-when": "3.5.0",
    "lint-staged": "12.1.3",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "release-it": "14.11.8",
    "rxjs": "~7.4.0",
    "sequelize": "6.12.0",
    "supertest": "6.1.6",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
    "typescript": "4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts}": [
      "npm run affected:lint",
      "npm run format:write"
    ]
  }
}

